<main class="main">
  <section id="car" class="mt-5">
    <div class="container">
      
      <section class="about section" id="about">
        
        <div
          class="jumbotron text-center"
          style="overflow-y: scroll; max-height: 70vh; background: #190222"
        >

      <form action="/save/newcar/" method="POST" 
      enctype="multipart/form-data"
      class="form__create_product">
        <div class="form-group">
          <label for="exampleInputEmail1">Name</label>
          <input type="text" class="form-control" id="exampleInputEmail1" name="name" aria-describedby="emailHelp" placeholder="Enter email" value="<%= car.name %>">
          <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
        <div class="form-group">
          <label for="exampleInputPassword1">Title</label>
          <input type="text" class="form-control" id="exampleInputPassword1" name="title" placeholder="title" value="<%= car.title %>">
          <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Length</label>
            <input type="text" class="form-control" name="length" id="exampleInputPassword1" placeholder="title" value="<%= car.length %>" >
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Fuel</label>
            <input type="text" class="form-control"name="fuel" id="exampleInputPassword1" value="<%= car.fuel %>" placeholder="title">
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Perfomance</label>
            <input type="text" class="form-control"name="perfomance" id="exampleInputPassword1" value="<%= car.perfomance %>" placeholder="title" >
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>  
        </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Price</label>
            <input type="text" class="form-control" name="price" id="exampleInputPassword1" value="<%= car.price %>" placeholder="title" >
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>             
        </div> 
          <div class="form-group">
            <label for="exampleInputPassword1">Filter</label>
            <input type="text" class="form-control" name="filter" id="exampleInputPassword1" value="<%= car.filter %>" placeholder="title">
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1">Image</label>
            <input type="file" name="img" id="myFileInput" onchange="UploadAndPreview(this)"/>
        </div> <br>
        <!-- <button type="submit" class="btn btn-primary">Create</button> -->
          <input value="<%= car.detailInfo %>" name="detailInfo" style="display: none;"/>
          <div class="containerFeature container" name = "text" id="result">
            
           
          </div>
        </div>

        </form>
        <div
          class="container-fluid"
          style="background: #190222; min-height: 30vh; position: absolute; bottom: 0"
        >
          <div class="container">
            <div class="row">
              <div class="col-12">
                <button class="btn btn-primary builderBtn" id="heading">
                  <i class="fa fa-header" aria-hidden="true"></i> Heading
                </button>
                <button class="btn btn-primary builderBtn" id="Paragraph">
                  <i class="fa fa-paragraph" aria-hidden="true"></i> Paragraph
                </button>
                <button class="btn btn-primary builderBtn" id="imgWithText">
                  <i class="fa fa-map-o" aria-hidden="true"></i> <br />
                  Image with Text
                </button>
                <button class="btn btn-primary builderBtn" id="saveInfo">
                  <i class="fa fa-map-o" aria-hidden="true"></i> <br />
                  Save
                </button>
              </div>
            </div>
          </div>
        </div>
        <div id="re"></div>
      </section>
      
      
    </div>
  </section>
  
</main>


<script src="/js/edit-product.js"></script>
